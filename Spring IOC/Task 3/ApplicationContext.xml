<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="shape" class="com.yash.Shape" abstract="true">
	</bean>
	<bean id="rectangle" class="com.yash.Rectangle" parent="shape"></bean>
	<bean id="parallelogram" class="com.yash.Parallelogram" parent="shape"></bean>
	<bean id="triangle" class="com.yash.Triangle" parent="shape"></bean>



</beans>